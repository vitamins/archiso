<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/3.4/menu">

<menu id="apps-term-menu" label="Terminals">
  <item label="LilyTerm">
    <action name="Execute">
      <command>lilyterm</command>
    </action>
  </item>
</menu>

<menu id="apps-net-menu" label="Internet">
  <item label="NetSurf">
    <action name="Execute">
      <command>netsurf</command>
    </action>
  </item>
</menu>

<menu id="apps-part-menu" label="Partitioning">
  <item label="GParted">
    <action name="Execute">
      <command>gparted</command>
    </action>
  </item>
</menu>

<menu id="root-menu" label="Openbox 3">
  <separator label="Applications" />
  <menu id="apps-net-menu"/>
  <menu id="apps-term-menu"/>
  <separator label="System" />
  <menu id="apps-part-menu"/>
  <separator />
  <item label="Log Out">
    <action name="Exit">
      <prompt>yes</prompt>
    </action>
  </item>
</menu>

</openbox_menu>
